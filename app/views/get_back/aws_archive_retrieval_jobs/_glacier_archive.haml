%tr.glacier_archive{:outro => :fade}
  %td{:style => 'width:120px;'}= archive.created_at.to_date.to_s(:default)
  = render :partial => "get_back/aws_archive_retrieval_jobs/#{archive.retrieval_status}", :locals => {:archive => archive, :fetch_fail => local_assigns[:fetch_fail] || false}
  %td.action_link#delete_archive{:onclick => "delete_archive(#{archive.id})", :style => 'padding-left: 20px;'} delete
